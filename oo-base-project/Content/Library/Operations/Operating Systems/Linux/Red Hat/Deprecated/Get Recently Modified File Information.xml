  <scriptlet id="a986f4b0-8a38-4b8b-a3ab-8e6ac37279a3">
    <annotation/>
    <name>Operation:Get Recently Modified File Information (23618650-d67e-4aff-a45c-c43ddb4e2098)</name>
    <script>/*
	This verifies that the directory entered ends with a '/' or appends one if it is needed
*/

// get the length
length = dir.length;
// it is zero based
if (length &gt; 0) length--;
// check the last character, if not a '/' then append a '/'
if (dir[length] != '/') dir = dir + "/";
// put it into the context
scriptletContext.putGlobal("dir",dir);



</script>
    <type>RHINO</type>
  </scriptlet>
