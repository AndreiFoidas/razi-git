      <scriptlet id="17e79b76-d5f5-4352-b18a-ff19edf3be3c">
        <annotation/>
        <name>Step:Remote Command Flow:Largest File (d6605d18-d819-4bd2-8a79-172823fce0c6)</name>
        <script>protocol = scriptletContext.get("protocol");
out = scriptletRawResult['stdOut'];
error = scriptletRawResult['exception'];
result = scriptletRawResult['Result'];
returnCode = scriptletRawResult['returnCode'];
stdErr = scriptletRawResult['stdErr'];

if (returnCode == -1)
{
    if ((error == null) || (error == ""))
    {
        if ((out == "") || (out == null))
            error = "Could not find largest file. Probable Cause: Invalid directory name"
        else
            error = "Could not find largest file." + stdErr;
     }
            
    scriptletContext.put("stdErr", error);
}
//Since ssh does not return the returnCode for failures for this command, 
//only stdErr is set 
else if ((stdErr != null) &amp;&amp; (stdErr != ""))
{
         scriptletContext.put("returnCode", "-1");
         scriptletContext.put("stdErr", stdErr);
}
else if ((protocol.indexOf("Telnet") != -1) &amp;&amp; (error == null))
{
    if ((result == null) || (result == ""))
    {
         scriptletContext.put("returnCode", "-1");
         scriptletContext.put("stdErr", "Could not find largest file. Probable Cause: Invalid directory name");
    }
    if (result.toLowerCase().indexOf("does not exist") != -1)
    {
         scriptletContext.put("returnCode", "-1");
         scriptletContext.put("stdErr", out);
     }
     if (result.toLowerCase().indexOf("file or directory in the path name does not exist") != -1)
     {
         scriptletContext.put("returnCode", "-1");
         scriptletContext.put("stdErr", out);
     }
      if (result.toLowerCase().indexOf("file access permissions do not allow the specified action") != -1)
     {
         scriptletContext.put("returnCode", "-1");
         scriptletContext.put("stdErr", out);      
      }
      if (result.toLowerCase().indexOf("permission denied") != -1)
      {
          scriptletContext.put("returnCode", "-1");
          scriptletContext.put("stdErr", out);   
      }
      if (result.toLowerCase().indexOf("usage") != -1)
      {
          scriptletContext.put("returnCode", "-1");
          scriptletContext.put("stdErr", out);   
      }
      if (result.toLowerCase().indexOf("not found") != -1)
      {
          scriptletContext.put("returnCode", "-1");
          scriptletContext.put("stdErr", out);   
      }
} </script>
        <type>RHINO</type>
      </scriptlet>
