  <scriptlet id="d03c4312-5ea9-41c8-875d-b1af96a3fb27">
    <annotation/>
    <name>Operation:Get Processor Count (1) (b3e0ecb2-3cbb-4998-9466-fa1d95e4cf42)</name>
    <script>// set up a default
scriptletResult = 0;

// make sure the command executed correctly
code = scriptletRawResult['Code'];
if (code == 0) {

   // split the data into lines
   data = scriptletRawResult['Output String'];
   splitdata = data.split(/\n/);

   // find the last row
   for (i = 1; ; i++) {
      // get the individual row
      row = splitdata[i];
      // if there is no data we are done
      if (row == null) break;
   }

   // the value in i is now one more than the number of processors
   i--;
   scriptletResult = i.toString();
}
</script>
    <type>RHINO</type>
  </scriptlet>
