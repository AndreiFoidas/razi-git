  <scriptlet id="e0f5f5da-d99a-4a56-ab22-fca1d637f78f">
    <annotation/>
    <name>Operation:Get Recently Modified File Information (0f927ce7-40a3-4913-9776-fb63fec81e79)</name>
    <script>/*
	This verifies that the directory entered ends with a '/' or appends one if it is needed
*/

// get the length
length = dir.length;
// it is zero based
if (length &gt; 0) length--;
// check the last character, if not a '/' then append a '/'
if (dir[length] != '/') dir = dir + "/";
// put it into the context
scriptletContext.putGlobal("dir",dir);



</script>
    <type>RHINO</type>
  </scriptlet>
