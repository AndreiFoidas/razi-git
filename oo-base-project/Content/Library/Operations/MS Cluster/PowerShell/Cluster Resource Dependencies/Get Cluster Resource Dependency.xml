      <scriptlet id="94f5a9d0-d78d-4d26-b858-baa9821d6908">
        <annotation/>
        <name>Step:Set Flow Variable Flow:Get Cluster Resource Dependency (411b613e-ed9c-420e-bc97-5ba7d8356282)</name>
        <script>var cmdlet = "Get-ClusterResourceDependency";

var resource = scriptletContext.get("resources");
var cluster = scriptletContext.get("cluster");
var guid = scriptletContext.get("guid");
var delim = scriptletContext.get("inputsDelimiter");

if ( (delim == "") || (typeof(delim) == 'undefined') )
    delim = ",";

if ( (resource != "") &amp;&amp; (typeof(resource) != 'undefined') )
{
    var resources = resource.split(delim);
    cmdlet += " -Resource "
    for (var i=0; i&lt;resources.length; i++)
        cmdlet += " \""+resources[i]+"\",";
    cmdlet = cmdlet.substring(0, cmdlet.length-1);        
}
    
if ( (cluster != "") &amp;&amp; (typeof(cluster) != 'undefined') )
    cmdlet += " -Cluster \"" + cluster + "\"";
    
if ( guid.toLowerCase() == 'true' )
        cmdlet += " -Guid ";
   
scriptletContext.put("cmdlets",cmdlet);</script>
        <type>RHINO</type>
