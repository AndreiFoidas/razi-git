      <scriptlet id="0de94479-f05e-48d7-9fdb-e554fd304ff5">
        <annotation/>
        <name>Step:Remote Command Flow:Check Process by PID (d4a1b3e4-67ad-45a1-ae57-92d6df6f9340)</name>
        <script>output = scriptletContext.get("mainResult");
PID = scriptletContext.get("PID");
protocol = scriptletContext.get("protocol");
returnCode = scriptletRawResult['returnCode'];
error = scriptletRawResult['exception'];
stdErr = scriptletRawResult['stdErr'];
result = scriptletRawResult['Result'];

if (returnCode == -1)
{
    if ((error == null) || (error == ""))
    {
        if ((stdErr == "" ) || (stdErr == null))
            error = "Could not get information";
        else  
           error = stdErr;
     }
     scriptletContext.put("stdErr", error);
}
if((protocol.indexOf("Telnet") != -1) &amp;&amp; (error == null))
{
    if ((output.indexOf(PID) == -1) || (output.indexOf("wrong PID number") != -1))
    {
        scriptletResponse = "failure";
        scriptletContext.put("returnCode", "-1");
        scriptletResult = PID + " - No such process";
    }
    if (result.toLowerCase().indexOf("file or directory in the path name does not exist") != -1)
    {
         scriptletContext.put("returnCode", "-1");
         scriptletContext.put("stdErr", result);
    }
    if (result.toLowerCase().indexOf("file access permissions do not allow the specified action") != -1)
    {
        scriptletContext.put("returnCode", "-1");
        scriptletContext.put("stdErr", result);      
    }
    if (result.toLowerCase().indexOf("used with invalid list") != -1)
    {
        scriptletContext.put("returnCode", "-1");
        scriptletContext.put("stdErr", result);      
    }
    if (result.toLowerCase().indexOf("permission denied") != -1)
      {
          scriptletContext.put("returnCode", "-1");
          scriptletContext.put("stdErr", result);   
      }
      if (result.toLowerCase().indexOf("not found") != -1)
      {
          scriptletContext.put("returnCode", "-1");
          scriptletContext.put("stdErr", result);   
      }
      if (result.toLowerCase().indexOf("usage") != -1)
      {
          scriptletContext.put("returnCode", "-1");
          scriptletContext.put("stdErr", result);   
      }
}

</script>
