      <scriptlet id="b4f48769-a162-4a8f-987f-708cc768d11e">
        <annotation/>
        <name>Step:Set Flow Variable Flow:Update Cluster IP Resource (bae62534-4bf1-4425-b897-4ceeef0ee92e)</name>
        <script>var cmdlet = "Update-ClusterIPResource";

var name = scriptletContext.get("name");
var cluster = scriptletContext.get("cluster");
var update = scriptletContext.get("update");

if ( (name != "") &amp;&amp; (typeof(name) != 'undefined') )
    cmdlet += " -Name \"" + name + "\"";

if ( (cluster != "") &amp;&amp; (typeof(cluster) != 'undefined') )
    cmdlet += " -Cluster \"" + cluster + "\"";

if ( (update != "") &amp;&amp; (typeof(update) != 'undefined') )
{
    if ( update.toLowerCase() == "release" ) 
        cmdlet += " -Release ";
    else if ( update.toLowerCase() == "renew" ) 
        cmdlet += " -Renew ";        
}

scriptletContext.put("cmdlets",cmdlet);</script>
